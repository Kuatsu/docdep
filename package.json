{
  "name": "docdep",
  "version": "0.1.1",
  "description": "Automagically generate beautiful documentation for your project's dependencies",
  "main": "lib/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "docdep": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts -c ./docdep.config.json",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "release": "release-it",
    "lint": "eslint --ext .ts src"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@release-it/conventional-changelog": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "release-it": "^15.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "chalk": "^5.2.0",
    "commander": "^11.0.0",
    "deepl-node": "^1.10.2",
    "i18next": "^23.2.3",
    "md-to-pdf": "^5.2.4",
    "package-json": "^8.1.1",
    "terminal-link": "^3.0.0"
  }
}
